#include <iostream>
#include <thread>
#include <stdio.h>
using namespace std;
void fun( )
{
    printf("Thread function Id = %d \n",this_thread::get_id());
}
int main() {
    printf("Main Thread id =  %d\n",this_thread::get_id());

    thread t = thread(fun);
    printf("t.get_id() = %d \n",t.get_id());

    t.join();

    return 0;
}
